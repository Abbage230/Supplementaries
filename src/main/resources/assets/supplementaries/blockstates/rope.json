

{
    "multipart": [
        {   "when":  {"OR": [
            {"north_connection": "block"},
            {"east_connection": "block"},
            {"south_connection": "block"},
            {"west_connection": "block"}
        ]},
            "apply": { "model": "supplementaries:block/rope_knot" }
        },
        {   "when": { "north_connection": "block" },
            "apply": { "model": "supplementaries:block/rope_connection_0", "uvlock": true}
        },
        {   "when": { "east_connection": "block" },
            "apply": { "model": "supplementaries:block/rope_connection_0", "y": 90, "uvlock": true}
        },
        {   "when": { "south_connection": "block" },
            "apply": { "model": "supplementaries:block/rope_connection_0", "y": 180, "uvlock": true}
        },
        {   "when": { "west_connection": "block" },
            "apply": { "model": "supplementaries:block/rope_connection_0", "y": 270, "uvlock": true}
        },
        {   "when": { "up": "true" },
            "apply": { "model": "supplementaries:block/rope_up"}
        },
        {   "when": { "down": "true" },
            "apply": { "model": "supplementaries:block/rope_down"}
        }
    ]
}